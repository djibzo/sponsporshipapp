<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sn.dev.sponsorshipapp.controllers.EleParCan">
   <children>
      <Button fx:id="parrainerBtn" layoutX="233.0" layoutY="293.0" mnemonicParsing="false" onAction="#parrainer" prefHeight="62.0" prefWidth="113.0" text="Parrainer">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <TableView fx:id="candidatTb" editable="true" layoutX="62.0" layoutY="40.0" onMouseClicked="#getData" prefHeight="237.0" prefWidth="476.0">
        <columns>
          <TableColumn fx:id="idCol" prefWidth="75.0" text="id" visible="false" />
          <TableColumn fx:id="nomCol" editable="false" prefWidth="75.0" sortable="false" text="Nom" />
            <TableColumn fx:id="prenomCol" prefWidth="75.0" text="Prenom" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button layoutX="495.0" layoutY="7.0" mnemonicParsing="false" onAction="#DECONNECTER" style="-fx-background-color: red;" text="Deconnexion" textAlignment="RIGHT" />
   </children>
</AnchorPane>
